<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.thegavners.CarlosMbenderaGSM">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.thegavners.CarlosMbenderaGSM.DirectChats" />
        <activity android:name="com.thegavners.CarlosMbenderaGSM.Chats" />

        <activity
            android:name="com.thegavners.CarlosMbenderaGSM.Profile"
            android:parentActivityName="com.thegavners.CarlosMbenderaGSM.Hub">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.thegavners.CarlosMbenderaGSM.Hub" />
        </activity>

        <activity
            android:name="com.thegavners.CarlosMbenderaGSM.Invite"
            android:parentActivityName="com.thegavners.CarlosMbenderaGSM.Hub">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.thegavners.CarlosMbenderaGSM.Hub" />
        </activity>
        <activity
            android:name="com.thegavners.CarlosMbenderaGSM.settings"
            android:parentActivityName="com.thegavners.CarlosMbenderaGSM.Hub">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.thegavners.CarlosMbenderaGSM.Hub" />
        </activity>
        <activity
            android:name="com.thegavners.CarlosMbenderaGSM.SavedPosts"
            android:parentActivityName="com.thegavners.CarlosMbenderaGSM.Hub">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.thegavners.CarlosMbenderaGSM.Hub" />
        </activity>
        <activity android:name="com.thegavners.CarlosMbenderaGSM.CreateProfile" />
        <activity
            android:name="com.thegavners.CarlosMbenderaGSM.Hub"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.thegavners.CarlosMbenderaGSM.ResetPassword"
            android:parentActivityName="com.thegavners.CarlosMbenderaGSM.SignIn">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.thegavners.CarlosMbenderaGSM.Hub" />
        </activity>
        <activity
            android:name="com.thegavners.CarlosMbenderaGSM.SignUp"
            android:parentActivityName="com.thegavners.CarlosMbenderaGSM.SignIn">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.thegavners.CarlosMbenderaGSM.SignIn" />
        </activity>
        <activity android:name="com.thegavners.CarlosMbenderaGSM.SignIn" />
        <activity
            android:name="com.thegavners.CarlosMbenderaGSM.SplashActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>